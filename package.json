{
  "name": "webGPSmap",
  "version": "0.0.2",
  "description": "View GEOJSON fiels on Openstreetmap",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint -c ../.eslintrc.json js/**",

    "static": "cp client/src/index.html dist && cp -r client/assets/. dist",

    "clean": "npm run clean:dist && rm -rf node_modules",
    "clean:dist": "rm -rf dist",

    "js": "browserify ./client/src/main.js > dist/bundle.js",

    "build": "npm install && mkdir dist && npm run static && npm run js",
    "debug": ""
  },
  "author": "Philipp Dippel & Dominik Wirtz",
  "license": "ISC",
  "dependencies": {
    "express": "^4.15.2"

  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "browserify": "^14.3.0"

  }
}
